			AllowedPenetrationDepth="0.25"
			DamageMult="100"
			ImpactType="Foliage"
			IsCapsuleCDT="false"
			LinearDamping="0.2"
			Mass="100.0"
			NoSoftContacts="true"
		/>
		<SmallTreeRoot
			AllowedPenetrationDepth="3"
			DamageMult="2"
			ImpactType="Foliage"
			IsCapsuleCDT="false"
			LinearDamping="0.2"
			Mass="50.0"
			NoSoftContacts="true"
		/>
		<SmallRock
			AttachToGround="false"
			DisableShadows="true"
			PlantingGroups="Bush,SmallTree"
			UseDirtTint="true"
		/>
		<SecondTrunk
			AllowedPenetrationDepth="3.0"
			ImpactType="Foliage"
			IsCapsuleCDT="true"
			LinearDamping="0.5"
			Mass="100"
		/>
		<MediumTreeRoot
			DamageMult="1"
			ImpactType="Foliage"
			IsCapsuleCDT="false"
			LinearDamping="0.2"
			Mass="600.0"
			NoSoftContacts="true"
		/>
		<LyingTrunk
			AngularDamping="2.0"
			DebrisType="DryTwigs"
			ImpactType="Foliage"
			LinearDamping="1.0"
			Mass="1000.0"
		/>
		<LyingBranch
			AllowedPenetrationDepth="2.0"
			AngularDamping="4.0"
			DebrisType="DryTwigs"
			ImpactType="Foliage"
			LinearDamping="1.0"
			Mass="50"
		/>
		<LyingTrunkStatic
			DebrisType="DryTwigs"
			ImpactType="Foliage"
			NoSoftContacts="true"
		/>
		<HugeTrunk
			DamageMult="0.5"
			ImpactType="Foliage"
			IsCapsuleCDT="true"
			LinearDamping="0.2"
			Mass="12000"
		/>
		<Flower
			AllowedPenetrationDepth="3.0"
			DamageMult="0.01"
			ImpactType="Foliage"
			IsCapsuleCDT="true"
			Mass="1"
		/>
		<FernRoot
			AllowedPenetrationDepth="3.0"
			AngularDamping="0.5"
			Collisions="None"
			DamageMult="0.01"
			ImpactType="Foliage"
			IsCapsuleCDT="true"
			Mass="20"
			NoSoftContacts="true"
		/>
		<FallingTreeRoot
			AllowedPenetrationDepth="0.5"
			DamageMult="0.4"
			Mass="700"
			AngularDamping="0.3"
		/>
		<Corn
			AllowedPenetrationDepth="3.0"
			DamageMult="0.2"
			ImpactType="Foliage"
			IsCapsuleCDT="true"
			Mass="1"
		/>
		<Cane
			AllowedPenetrationDepth="3.0"
			DamageMult="0.01"
			ImpactType="Foliage"
			IsCapsuleCDT="true"
			Mass="5"
		/>
		<BushRoot
			AllowedPenetrationDepth="3.0"
			AngularDamping="0.5"
			CenterOfMassOffset="(0; -0.3; 0)"
			Collisions="None"
			DamageMult="0.01"
			ImpactType="Foliage"
			IsCapsuleCDT="true"
			LinearDamping="0.5"
			Mass="40"
			NoSoftContacts="true"
		/>
		<BushBranch
			AllowedPenetrationDepth="3.0"
			DamageMult="0.01"
			ImpactType="Foliage"
			IsCapsuleCDT="true"
			Mass="3"
		/>
		<BranchWithLeaves
			AllowedPenetrationDepth="2.0"
			DamageMult="0.01"
			ImpactType="Foliage"
			IsCapsuleCDT="true"
			LinearDamping="0.1"
			Mass="50"
		/>
		<BranchSoft
			AllowedPenetrationDepth="2.0"
			DamageMult="0.01"
			ImpactType="Foliage"
			IsCapsuleCDT="true"
			LinearDamping="0.1"
			Mass="25"
		/>
